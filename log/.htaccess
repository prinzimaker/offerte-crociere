# Rewrite rules per il Log Viewer (msclog.fttn.it)
#
# Redirige tutte le richieste a public/index.php

RewriteEngine On

# Non riscrivere file e directory esistenti
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirige tutto a public/index.php
RewriteRule ^(.*)$ public/index.php [QSA,L]
